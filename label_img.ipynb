   "source": [
    "import cv2\n",
    "cap=cv2.VideoCapture('mycarplate.mp4')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "f=0\n",
    "c=0\n",
    "while True:\n",
    "    ret,frame=cap.read()\n",
    "    if not ret:\n",
    "        break\n",
    "    if f%6==0:\n",
    "        c+=1\n",
    "        cv2.imwrite(f'./images/image_{c}.jpg',frame)\n",
    "    f+=1\n",
    "cv2.destroyAllWindows()"
   ]
  }
